{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "modulePathIgnorePatterns": ["<rootDir>/dist/", "<rootDir>/build/"],
  "testPathIgnorePatterns": ["<rootDir>/dist/", "<rootDir>/build/"],
  "moduleNameMapper": {
    "(.+)\\.js": "$1"
  },
  "setupFilesAfterEnv": ["./jest.setup.ts"],
  "globals": {
    "PropertiesService": {}
  }
}
