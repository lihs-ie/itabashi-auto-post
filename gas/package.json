{
  "name": "itabashi-auto-post-gas",
  "version": "0.0.0",
  "description": "",
  "main": "build/index.js",
  "license": "Apache-2.0",
  "keywords": [],
  "scripts": {
    "clean": "rimraf build dist",
    "lint": "eslint --fix --no-error-on-unmatched-pattern src/",
    "bundle": "rollup --no-treeshake -c rollup.config.mjs",
    "build:dev": "npm run clean && BUILD_ENV=development rollup --no-treeshake -c rollup.config.mjs && ncp appsscript.json dist-dev/appsscript.json",
    "build:prd": "npm run clean && BUILD_ENV=production rollup --no-treeshake -c rollup.config.mjs && ncp appsscript.json dist-prd/appsscript.json",
    "license": "license-check-and-add add -f license-config.json",
    "test": "jest test/ --passWithNoTests --detectOpenHandles",
    "deploy:dev": "npm run lint && npm run test && npm run build:dev && ncp .clasp-dev.json .clasp.json && clasp push -f && source .env.development && clasp deploy --deploymentId $DEPLOYMENT_ID",
    "deploy:prd": "npm run lint && npm run test && npm run build:prd && ncp .clasp-prod.json .clasp.json && clasp push -f && source .env.production && clasp deploy --deploymentId $DEPLOYMENT_ID"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@google/clasp": "^2.5.0",
    "@types/google-apps-script": "^1.0.91",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "gts": "^6.0.2",
    "jest": "^29.7.0",
    "license-check-and-add": "^4.0.5",
    "ncp": "^2.0.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.30.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-license": "^3.5.3",
    "rollup-plugin-prettier": "^4.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "type-fest": "^4.33.0",
    "typescript": "^5.7.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@types/node-fetch": "^2.6.12",
    "@types/sync-fetch": "^0.4.3",
    "fetch-sync": "^2.0.0",
    "hash.js": "^1.1.7",
    "immutable": "^5.0.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-extended": "^4.0.0-beta1",
    "jest-to-equal-type": "^0.1.0",
    "sync-fetch": "^0.6.0-2",
    "sync-request": "^6.1.0",
    "ts-jest": "^29.2.5",
    "uuid": "^11.0.5"
  }
}
