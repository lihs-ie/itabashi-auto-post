{
  "env": {
    "browser": true,
    "node": true,
    "es2020": true
  },
  "extends": [
    "./node_modules/gts",
    "prettier",
    "plugin:storybook/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": [
      "./tsconfig.json",
      "./tests/tsconfig.json"
    ]
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "@typescript-eslint/no-explicit-any": "warn"
  }
}